{
  "default": {
    "extensions": [".json", ".js", ".ts", ".jsx", ".tsx"],
    "exclusions": ["node_modules/", "build/"]
  },
  "docs": {
    "extensions": [".md"],
    "first_file": "README.md"
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore"]
  }
}
